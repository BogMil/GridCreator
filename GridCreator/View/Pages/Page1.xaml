<Page x:Class="GridCreator.View.Pages.Page1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GridCreator"
             mc:Ignorable="d" 
             d:DesignHeight="800" d:DesignWidth="800" >

    <ScrollViewer>
        <StackPanel>
            <TextBlock>Test page 1</TextBlock>
            <TextBox Text="{Binding TableId, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"></TextBox>
            <TextBox></TextBox>
            <TextBlock Text="{Binding NavigatorId, Mode=TwoWay}"></TextBlock>
            <TextBlock Text="Test" Margin="0 700 0 0 "></TextBlock>

        </StackPanel>
    </ScrollViewer>
</Page>
