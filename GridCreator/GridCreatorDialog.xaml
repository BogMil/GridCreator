<Window x:Class="GridCreator.GridCreatorDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GridCreator"
             
             mc:Ignorable="d" 
             d:DesignHeight="800" d:DesignWidth="800">
    <Window.Resources>
        <local:testViewModel x:Key="vm"/>
    </Window.Resources>
    <Grid Height="700">
        <StackPanel>
            <TextBlock Height="50" Margin="10 10">asd</TextBlock>
            <Button Height="50" Margin="10 10" Command="{Binding TestCommand, Source={StaticResource vm}}">click me</Button>
        </StackPanel>

        <Frame x:Name="testFrame" NavigationUIVisibility="Hidden" Height="200" Margin="10 150 10 50"/>
        <StackPanel Margin="0 500 0 0">
            <Button Click="Button_Click">
                Page 1
            </Button>
            <Button Click="Button_Click_1">
                Page 2
            </Button>
        </StackPanel>
    </Grid> 
</Window>
